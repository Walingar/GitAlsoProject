<idea-plugin>
    <id>com.jetbrains.GitAlso</id>
    <name>GitAlso</name>
    <version>0.0.5</version>
    <vendor email="walingar@mail.ru" url="http://www.jetbrains.com">JetBrains</vendor>

    <description>This plugin predicts files which developer may forgot to change or add to CVS.</description>

    <idea-version since-build="173.0"/>

    <project-components>
        <component>
            <implementation-class>GitAlsoComponent</implementation-class>
        </component>
    </project-components>

    <extensions defaultExtensionNs="com.intellij">
        <checkinHandlerFactory implementation="GitAlsoCheckinHandlerFactory"/>
        <projectService serviceInterface="GitAlsoService" serviceImplementation="GitAlsoService">

        </projectService>
    </extensions>

    <actions>
    </actions>
</idea-plugin>