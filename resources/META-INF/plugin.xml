<idea-plugin>
    <id>com.jetbrains.GitAlso</id>
    <name>GitAlso</name>
    <version>0.0.6</version>
    <vendor email="walingar@mail.ru" url="http://www.jetbrains.com">JetBrains</vendor>

    <description>This plugin predicts files which developer may forgot to modify or add to VCS.</description>

    <idea-version since-build="183.1182"/>

    <depends>com.intellij.modules.lang</depends>

    <extensions defaultExtensionNs="com.intellij">
        <vcsLogIndexService implementation="com.jetbrains.gitalso.repository.LogIndexService"/>
        <checkinHandlerFactory implementation="com.jetbrains.gitalso.commitHandle.GitAlsoCheckinHandlerFactory"/>
    </extensions>

    <actions>
    </actions>
</idea-plugin>